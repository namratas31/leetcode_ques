 int removeDuplicates(vector<int>& nums) {
        
        int count=0;
        int i;
        
        for(i=0;i<nums.size()-1;i++)
        {
            if(nums[i+1]!=nums[i])
            {
                count++;
                nums[count-1]=nums[i];
            }
        }
        
        count++;
        nums[count-1]=nums[i];
        
        return count;
    }
