 int singleNumber(vector<int>& nums) {
        
        unordered_map<int,int>mp;
        int ans;
        
        for(auto i:nums)
        {
            mp[i]++;
        }
        
        for(auto i:mp)
        {
            if(i.second!=2)
            {
                ans=i.first;
                break;
            }
        }
        
        return ans;
    }
