 vector<vector<int>> transpose(vector<vector<int>>& matrix) {
        
        int i,j;
        vector<vector<int>>ans(matrix[0].size(),vector<int>(matrix.size()));
              
        for(i=0;i<matrix.size();i++)
        {
            for(j=0;j<matrix[0].size();j++)
            {
                ans[j][i]=matrix[i][j];
            }
        }
        
        return ans;
    }
