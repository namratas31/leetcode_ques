int lengthOfLastWord(string s) {
        
        int i=s.length()-1,count=0;
        
        while(i>=0)
        {
            if(s[i]!=' ')
                break;
            i--;
        }
        
        while(i>=0)
        {
            if(s[i]==' ')
            {
                break;
            }
            count++;
            i--;    
        }
        
        return count;
    }
